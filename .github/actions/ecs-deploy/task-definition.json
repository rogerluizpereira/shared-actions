{
  "family": "task-family",
  "containerDefinitions": [
      {
          "name": "container-name",          
          "image": "docker-image-name:latest",
          "cpu": 128,
          "memory": 128,
          "essential": true,
          "portMappings": [
              {
                  "containerPort": 9097,
                  "hostPort": 0,
                  "protocol": "tcp"
              }
          ],
          "environment": [
              {
                  "name": "ENVIRONMENT",
                  "value": "env-type"
              }
          ],
          "mountPoints": [],
          "volumesFrom": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-group": "/ecs/task-family",
                  "mode": "non-blocking",
                  "awslogs-create-group": "true",
                  "max-buffer-size": "25m",
                  "awslogs-region": "default-region",
                  "awslogs-stream-prefix": "ecs"
              }
          },
          "systemControls": []
      }
  ],
  "taskRoleArn": "arn:aws:iam::account:role/task-role-name",
  "executionRoleArn": "arn:aws:iam::account:role/execution-role-name",
  "volumes": [],
  "placementConstraints": [],
  "requiresCompatibilities": [
      "EC2"
  ],
  "cpu": "128",
  "memory": "128",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  },
  "tags": [
      {
          "key": "managed-by",
          "value": "manage-tool"
      },
      {
          "key": "environment",
          "value": "env-type"
      }
  ]
}